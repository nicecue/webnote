{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "removeComments": true,
    "strictNullChecks": false,
    "outDir": "build",
    "lib": [
      "es6",
      "es7",
      "dom"
    ],
    "baseUrl": "src",
    "paths": {
      "Components": [
        "app/components"
      ],
      "Containers": [
        "app/containers"
      ],
      "Stores" : [
        "app/stores"
      ],
      "Models" : [
        "app/models"
      ]
    }
  },
  "exclude": [
    "dist",
    "build",
    "node_modules"
  ]
}